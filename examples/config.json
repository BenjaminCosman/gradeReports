            "items": []
        },
        {
            "file": "examples/data/clickerRegistrations.csv",
            "attributes": {
                "Your PID": "Student ID",
                "Your iclicker ID": "Clicker ID"
            },
            "items": []
        },
        {
            "file": "examples/data/UploadFile.csv",
